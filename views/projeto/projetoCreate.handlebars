<form action="/projetoCreate" method="post">
    <div class="form-group">
        <label for="inputNome">Nome</label>
        <input type="text" class="form-control" id="inputNome" name="nome">
    </div>
    <div class="form-group">
        <label for="inputResumo">Resumo</label>
        <input type="text" class="form-control" id="inputResumo" name="resumo">
    </div>
    <div class="form-group">
        <label for="inputLinkExterno">Link Externo</label>
        <input type="text" class="form-control" id="inputLinkExterno" name="link_externo">
    </div>
    <div class="form-group">
        <label for="palavraChaveIds">Palavras Chave:</label>
        <select name="palavraChaveIds" multiple class="form-control">
            {{#each palavraChave}}
                <option value="{{id}}">
                    {{nome}}
                </option>
            {{/each}}
        </select>
    </div>
    <div class="form-group">
        <label for="usuarioIds">Usu√°rios:</label>
        <select name="usuarioIds" multiple class="form-control">
            {{#each usuarios}}
                <option value="{{this.id}}">
                    {{this.login}}
                </option>
            {{/each}}
        </select>
    </div>
    <button type="submit" class="btn btn-primary">Cadastrar</button>
</form>